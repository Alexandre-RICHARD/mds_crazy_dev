{
  "type": "module",
  "scripts": {
    "start": "vite --open --mode dev",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "check-types": "tsc --noEmit",
    "update": "pnpm update || yarn upgrade || npm update",
    "build": "tsc && vite build --mode prod",
